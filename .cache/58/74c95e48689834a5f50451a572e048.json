nerated":{"line":1,"column":4267}},{"source":"style.css","name":null,"original":{"line":379,"column":0},"generated":{"line":1,"column":4285}},{"source":"style.css","name":null,"original":{"line":381,"column":0},"generated":{"line":1,"column":4286}},{"source":"style.css","name":null,"original":{"line":382,"column":2},"generated":{"line":1,"column":4319}},{"source":"style.css","name":null,"original":{"line":383,"column":0},"generated":{"line":1,"column":4331}}],"sources":{"style.css":"/*\n * ========\n * Contents\n * ========\n *\n * - Variables\n *   css variables / custom props for reusable values\n *\n * - Type\n *   global type rules\n *\n * - Layout\n *   styles for grid & layout items\n *\n * - Appearance\n *   colors, borders, etc.\n *\n * - App Elements\n *   navbar, title, content\n */\n\n/* Variables */\n:root {\n  --hue-bg: 60deg;\n  --sat-bg: 33%;\n  --lum-bg: 90%;\n  --lum-bg-lt: 96%;\n\n  --hue-fg: 203deg;\n  --sat-fg: 13%;\n  --lum-fg: 16%;\n\n  --hue-acc-blue: 159deg;\n  --sat-acc-blue: 50%;\n  --lum-acc-blue: 82%;\n\n  --hue-acc-green: 107deg;\n  --sat-acc-green: 41%;\n  --lum-acc-green: 78%;\n\n  --hue-acc-yellow: 35deg;\n  --sat-acc-yellow: 90%;\n  --lum-acc-yellow: 80%;\n\n  --color-bg: hsl( var( --hue-bg )\n                 , var( --sat-bg )\n                 , var( --lum-bg )\n                 );\n\n  --color-bg-lt: hsl( var( --hue-bg )\n                    , var( --sat-bg )\n                    , var( --lum-bg-lt )\n                    );\n\n  --color-fg: hsl( var( --hue-fg )\n                 , var( --sat-fg )\n                 , var( --lum-fg )\n                 );\n\n  --color-accent-blue: hsl( var( --hue-acc-blue )\n                          , var( --sat-acc-blue )\n                          , var( --lum-acc-blue )\n                          );\n\n  --color-accent-green: hsl( var( --hue-acc-green )\n                           , var( --sat-acc-green )\n                           , var( --lum-acc-green )\n                           );\n\n  --color-accent-yellow: hsl( var( --hue-acc-yellow )\n                            , var( --sat-acc-yellow )\n                            , var( --lum-acc-yellow )\n                            );\n}\n\n/* Type */\nh1 {\n  font-family: 'GlamourExtended';\n  font-size: 40pt;\n  font-variant-ligatures: discretionary-ligatures;\n}\n\nh1.huge {\n  font-size: 48pt;\n}\n\nh2, h3, h4, h5, p, span, body, button {\n  font-family: 'Inter';\n}\n\nbody {\n  font-synthesis: none !important;\n  font-feature-settings: \"kern\" 1;\n  font-kerning: normal;\n}\n\nbutton {\n  font-weight: 500;\n}\n\n/* Layout */\nh1, h2, h3, h4, h5, p, body {\n  margin: 0;\n}\n\n.app-root {\n  display: grid;\n  grid: \" title navbar navbar   \" auto\n        \" lg-1  lg-1   .     \" auto\n        \" lg-1  lg-1   .     \" auto\n      /   1fr   1fr    1fr\n    ;\n\n  grid-gap: 32px;\n  grid-auto-rows: 256px;\n}\n\n.grid-name {\n  grid-area: name;\n}\n\n.grid-content-lg, .lg {\n  grid-area: lg-1;\n}\n\n.flex {\n  display: flex;\n}\n\n.flex.inline {\n  display: inline-flex;\n}\n\n.flex.stretch-children-main-axis {\n  align-content: stretch;\n}\n\n.flex.center-children-sec-axis {\n  align-items: center;\n}\n\n.flex.center-children-main-axis {\n  justify-content: center;\n}\n\n.flex.vert {\n  flex-direction: column;\n}\n\n/* Appearance */\nbody {\n  background: var( --color-bg );\n}\n\nimg.plant {\n  width: 100%;\n  transform: scaleY( -1 );\n  opacity: 0.5;\n  position: fixed;\n  z-index: -1;\n  top: -40%;\n  left: 0;\n}\n\nbutton {\n  cursor: pointer;\n}\n\n.bg-green {\n  background: var( --color-accent-green );\n}\n\n.bg-yellow {\n  background: var( --color-accent-yellow );\n}\n\n.bg-blue {\n  background: var( --color-accent-blue );\n}\n\nbutton {\n  --hover-lum-diff: -5%;\n  --active-lum-diff: -10%;\n}\n\nbutton.bg-green:hover {\n  background: hsl( var( --hue-acc-green )\n                 , var( --sat-acc-green )\n                 , calc( var( --lum-acc-green ) + var( --hover-lum-diff ) )\n                 );\n}\n\nbutton.bg-green:active {\n  background: hsl( var( --hue-acc-green )\n                 , var( --sat-acc-green )\n                 , calc( var( --lum-acc-green ) + var( --active-lum-diff ) )\n                 );\n}\n\nbutton.bg-yellow:hover {\n  background: hsl( var( --hue-acc-yellow )\n                 , var( --sat-acc-yellow )\n                 , calc( var( --lum-acc-yellow ) + var( --hover-lum-diff ) )\n                 );\n}\n\nbutton.bg-yellow:active {\n  background: hsl( var( --hue-acc-yellow )\n                 , var( --sat-acc-yellow )\n                 , calc( var( --lum-acc-yellow ) + var( --active-lum-diff ) )\n                 );\n}\n\nbutton.bg-blue:hover {\n  background: hsl( var( --hue-acc-blue )\n                 , var( --sat-acc-blue )\n                 , calc( var( --lum-acc-blue ) + var( --hover-lum-diff ) )\n                 );\n}\n\nbutton.bg-blue:active {\n  background: hsl( var( --hue-acc-blue )\n                 , var( --sat-acc-blue )\n                 , calc( var( --lum-acc-blue ) + var( --active-lum-diff ) )\n                 );\n}\n\n/* App Elements */\n.app-root {\n  padding: 32px;\n}\n\n.app-root * {\n  box-sizing: border-box;\n}\n\n.app-title {\n  margin-left: 16px;\n  grid-area: title;\n  justify-self: start;\n}\n\n.app-navbar {\n  grid-area: navbar;\n}\n\n.app-navbar > * {\n  margin-left: 32px;\n}\n\n.app-navbar :first-child {\n  margin-left: 0;\n}\n\n.app-navbar button {\n  display: inline-block;\n  padding: 12px;\n  flex-grow: 1;\n  border: none;\n  border-radius: 100px;\n  font-weight: 700;\n  font-size: 16pt;\n}\n\n.app-navbar button.selected {\n  /* Selected title item is 3 times width of non-selected */\n  flex-grow: 3;\n}\n\n.app-navbar h1 {\n  /* Don't grow or shrink navbar title */\n  flex-basis: content;\n}\n\nh1, h2, h3, h4, h5, h6, div, span, button {\n  color: var( --color-fg );\n}\n\n.ribbon {\n  isolation: isolate;\n  position: relative;\n}\n\n.ribbon::before {\n  content: '';\n  position: absolute;\n  border-radius: 1000px;\n  height: 50%;\n  width: 115%;\n  left: -5%;\n  bottom: 5%;\n  z-index: -1;\n}\n\n.ribbon-yellow::before {\n  background: var( --color-accent-yellow );\n}\n\n.ribbon-blue::before {\n  background: var( --color-accent-blue );\n}\n\n.ribbon-green::before {\n  background: var( --color-accent-green );\n}\n\n.card.lg {\n  --card-basis: 32px;\n}\n\n.card:not( .lg ) {\n  --card-basis: 24px;\n}\n\n.card {\n  padding: var( --card-basis );\n  align-self: start;\n  border-radius: 24px;\n}\n\n.card.yellow:not( .lg ) {\n  border-color: var( --color-accent-yellow );\n}\n\n.card.blue:not( .lg ) {\n  border-color: var( --color-accent-blue );\n}\n\n.card.green:not( .lg ) {\n  border-color: var( --color-accent-green );\n}\n\n.card h1 {\n  align-self: center;\n}\n\n.card code {\n  font-size: 14pt;\n}\n\n.card h1.ribbon::before {\n  height: 40%;\n  width: 125%;\n}\n\n.card.lg h1 {\n  font-size: 56pt;\n  margin: 16px;\n}\n\n.card.lg h2 {\n  font-size: 24pt;\n}\n\n.card.lg p {\n  font-size: 14pt;\n}\n\n.card .card-content {\n  padding: var( --card-basis );\n  border-radius: calc( var( --card-basis ) / 2 );\n  background: var( --color-bg-lt );\n}\n\n.card .card-content {\n  margin-top: calc( var( --card-basis ) );\n}\n\n.card .card-content div > *:not(div) {\n  margin-top: calc( var( --card-basis ) * 0.25 );\n}\n\n.card .card-content div {\n  margin-top: calc( var( --card-basis ) * 0.5 );\n}\n\n.card .card-content > * {\n  margin-top: inherit;\n}\n\n.card .card-content :first-child {\n  margin-top: 0;\n}\n"},"lineCount":null}},"error":null,"hash":"5c7c704adb0eb2e8622b01a1a629e8b9","cacheData":{"env":{}}}